<%= javascript_include_tag "main" %>
<%= javascript_include_tag "admin/users_edit" %>



<form class="form-horizontal" role="form" id="filter_form" name="filter_form">

  <div class="form-group">
    <label for="login" class="col-sm-2 control-label">Login</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="login" id="login" value="<%= @form_data[:user].login  %>">
    </div>
  </div>

  <div class="form-group">
    <label for="username" class="col-sm-2 control-label">username</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="username" id="username" value="<%= @form_data[:user].username  %>">
    </div>
  </div>

  <div class="form-group">
    <label for="email" class="col-sm-2 control-label">email</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="email" id="email" value="<%= @form_data[:user].email  %>">
    </div>
  </div>

  <div class="form-group">
    <label for="pwd" class="col-sm-2 control-label">pwd</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="pwd" id="pwd" value="<%= @form_data[:user].pwd  %>">
    </div>
  </div>

  <div class="form-group">
    <label for="user_login" class="col-sm-2 control-label">qiwi</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="qiwi" id="qiwi" value="<%= @form_data[:user].qiwi  %>">
    </div>
  </div>

  <div class="form-group">
    <label for="user_login" class="col-sm-2 control-label">Login</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="user_login" id="user_login" value="<%= @form_data[:user].login  %>">
    </div>
  </div>

  <div class="form-group">
    <label for="adm" class="col-sm-2 control-label">adm</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="adm" id="adm" value="<%= @form_data[:user].adm  %>">
    </div>
  </div>

  <div class="form-group">
    <label for="active" class="col-sm-2 control-label">active</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="active" id="active" value="<%= @form_data[:user].active  %>">
    </div>
  </div>

  <div class="form-group">
    <label for="price_sms" class="col-sm-2 control-label">price_sms</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="price_sms" id="price_sms" value="<%= @form_data[:user].price_sms  %>">
    </div>
  </div>

  <div class="form-group">
    <label for="price_voice" class="col-sm-2 control-label">price_voice</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="price_voice" id="price_voice" value="<%= @form_data[:user].price_voice  %>">
    </div>
  </div>

  <div class="form-group">
    <label for="service_sms_id" class="col-sm-2 control-label">service_sms_id</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="service_sms_id" id="service_sms_id" value="<%= @form_data[:user].service_sms_id  %>">
    </div>
  </div>

  <div class="form-group">
    <label for="service_voice_id" class="col-sm-2 control-label">service_voice_id</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="service_voice_id" id="service_voice_id" value="<%= @form_data[:user].service_voice_id  %>">
    </div>
  </div>


  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-success" onclick="save_user()">Сохранить</button>
      <button type="button" class="btn btn-info" >Отменить</button>
    </div>
  </div>
</form>